const categorias = ["Celulares", "Audio", "Electrónica", "Computadores", "TV", "Consolas", "Hogar", "Ropa", "Accesorios", "Juguetería"];
const marcas = ["Apple", "Samsung", "Xiaomi", "Sony", "JBL", "Logitech", "Razer", "HP", "Acer", "Asus", "Lenovo", "LG", "Adidas", "Nike", "Hasbro", "Lego", "Canon", "Nikon", "GoPro", "DJI"];

let docs = [];

for (let i = 0; i < 100; i++) {
  docs.push({
    nombre: productos[i % productos.length],
    categoria: categorias[Math.floor(Math.random() * categorias.length)],
    precio: Math.floor(Math.random() * 400000) + 50000,
    stock: Math.floor(Math.random() * 50) + 1,
    marca: marcas[Math.floor(Math.random() * marcas.length)],
    rating: (Math.random() * 5).toFixed(1),
    vendidos: Math.floor(Math.random() * 5000),
    fecha_ingreso: new Date()
  });
}

db.productos.insertMany(docs);
